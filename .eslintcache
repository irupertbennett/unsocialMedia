        </div>\r\n                    <div className=\"text-center\">\r\n                        <input id=\"relationshipStatus\" className=\"form-control\" value={this.state.relationshipStatus} onChange={ (e) => this.updateState(e)}></input>\r\n                        <p>Relationship Status</p>\r\n                    </div>\r\n                    <div className=\"text-center pt-3\">\r\n                        <button className=\"btn\" onClick={this.toggleDetails}>Cancel</button>\r\n                        <button className=\"btn\" onClick={this.submitDetails}>Save</button>\r\n                    </div>\r\n                </div>\r\n            )\r\n        }\r\n        else{\r\n            return (\r\n                <div id={ profile.id } className=\"user-info px-5 py-4 text-center\">\r\n                    <div className=\"text-center\">\r\n                        <h5>{ profile.city ? profile.city : \"Unknown\" }</h5>\r\n                        <p>City</p>\r\n                    </div>\r\n                    <div className=\"text-center\">\r\n                        <h5>{ profile.email ? profile.email : \"Unknown\"  }</h5>\r\n                        <p>E-mail</p>\r\n                    </div>\r\n                    <div className=\"text-center\">\r\n                        <h5>{ profile.relationshipStatus ? profile.relationshipStatus : \"Unknown\"  }</h5>\r\n                        <p>Relationship Status</p>\r\n                    </div>\r\n                    <div className=\"text-center pt-3\">\r\n                        <button className=\"btn\" onClick={this.toggleDetails}>Edit details</button>\r\n                    </div>\r\n                </div>\r\n            )\r\n        }\r\n    }\r\n\r\n    render(){\r\n        const { profile } = this.props\r\n        if(profile){\r\n            return ( \r\n                <div>\r\n                    { this.getDetailsDisplay(profile) }\r\n                </div>\r\n            );\r\n        } else {\r\n            return <p>Loading...</p>\r\n        }\r\n    }\r\n}\r\n \r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        auth: state.firebase.auth,\r\n        profile: state.firebase.profile\r\n    }\r\n}\r\n\r\nconst mapDispatchtoProps = (dispatch) => {\r\n    return {\r\n        updateDetails: (newDetails) => dispatch(updateDetails(newDetails))\r\n    }\r\n}\r\nexport default connect(mapStateToProps, mapDispatchtoProps)(UserInfo)","C:\\Users\\irupe\\OneDrive\\Documents\\React\\unsocialMedia\\unsocial-media\\src\\profile\\bio.js",[],"C:\\Users\\irupe\\OneDrive\\Documents\\React\\unsocialMedia\\unsocial-media\\src\\profile\\profileIntro.js",[],"C:\\Users\\irupe\\OneDrive\\Documents\\React\\unsocialMedia\\unsocial-media\\src\\ReduxStore\\Reducers\\postReducer.js",[],"C:\\Users\\irupe\\OneDrive\\Documents\\React\\unsocialMedia\\unsocial-media\\src\\config\\fbConfig.js",[],{"ruleId":"122","severity":1,"message":"123","line":5,"column":8,"nodeType":"124","messageId":"125","endLine":5,"endColumn":23},{"ruleId":"126","severity":1,"message":"127","line":34,"column":3,"nodeType":"128","messageId":"129","endLine":34,"endColumn":9},{"ruleId":"130","replacedBy":"131"},{"ruleId":"132","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":17,"column":59,"nodeType":"136","endLine":17,"endColumn":140},{"ruleId":"137","severity":1,"message":"138","line":17,"column":59,"nodeType":"136","endLine":17,"endColumn":140},{"ruleId":"139","severity":1,"message":"140","line":12,"column":39,"nodeType":"141","messageId":"142","endLine":12,"endColumn":41},{"ruleId":"122","severity":1,"message":"143","line":1,"column":8,"nodeType":"124","messageId":"125","endLine":1,"endColumn":14},{"ruleId":"144","severity":1,"message":"145","line":73,"column":17,"nodeType":"136","endLine":73,"endColumn":44},{"ruleId":"122","severity":1,"message":"146","line":4,"column":8,"nodeType":"124","messageId":"125","endLine":4,"endColumn":16},"no-unused-vars","'reportWebVitals' is defined but never us